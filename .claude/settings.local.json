{
  "$schema": "https://json.schemastore.org/claude-code-settings.json",
  "permissions": {
    "allow": [
      "Bash(mkdir:*)",
      "Bash(mv:*)",
      "Bash(ls:*)",
      "Bash(rm:*)",
      "Bash(grep:*)",
      "Bash(for:*)",
      "Bash(do if [ -f \"js/$file\" ])",
      "Bash(then echo \"✓ js/$file\")",
      "Bash(else echo \"✗ js/$file MISSING\")",
      "Bash(fi)",
      "Bash(done)"
    ],
    "deny": []
  }
}